{
  "name": "GLY",
  "appid": "__UNI__GLYAPP",
  "description": "GLY Quantum Investment Platform",
  "versionName": "1.0.0",
  "versionCode": "100",
  "transformPx": false,
  "app-plus": {
    "usingComponents": true,
